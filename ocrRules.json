{
  "version": 1,
  "rules": {
    "Date": {
      "type": "format",
      "strictCase": false,
      "allowEmpty": false,
      "config": {
        "formats": [
          "(nn|n).(nn|n).nnnn (nn|n):nn"
        ]
      }
    },
    "Milk Yeild-MY": {
      "type": "format",
      "strictCase": false,
      "allowEmpty": true,
      "config": {
        "formats": [
          "(nnn|nn|n).n"
        ]
      }
    },
    "Milk Yield Expected-MYE": {
      "type": "format",
      "strictCase": false,
      "allowEmpty": true,
      "config": {
        "formats": [
          "(nnn|nn|n).n"
        ]
      }
    },
    "Visit Result-VR": {
      "type": "enum",
      "strictCase": false,
      "allowEmpty": false,
      "config": {
        "values": [
          "Successful",
          "Milk Interval not Passed",
          "Teats not found",
          "Connection attempts RF",
          "Connection attempts LF",
          "Connection attempts RR",
          "Connection attempts LR",
          "Connection time",
          "Dead milk time LF",
          "Dead milk time LR",
          "Dead milk time RR",
          "Dead milk time RF",
          "Stopped by user"
        ]
      }
    }
  }
}